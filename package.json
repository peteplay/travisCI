{
  "name": "travis-simple",
  "version": "1.0.0",
  "description": "Cypress Cucumber with Travis CI",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:electron": "cypress run --browser electron",
    "cy:run:edge": "cypress run --browser edge",
    "cy:dashboard": "cypress run --record",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info",
    "cy:run:browserstack": "browserstack-cypress run",
    "ci:teamcity-cy-run": "cypress run --reporter mocha-teamcity-reporter"
  },
  "keywords": [
    "TravisCI"
  ],
  "author": "Peter",
  "license": "ISC",
  "dependencies": {
    "cypress": "^5.5.0",
    "cypress-cucumber-preprocessor": "^4.0.0",
    "cypress-image-snapshot": "^3.1.1",
    "mocha": "^8.2.1",
    "mocha-teamcity-reporter": "^3.0.0",
    "prettier": "^2.1.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "devDependencies": {
    "mochawesome": "^6.2.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.1.0",
    "start-server-and-test": "^1.11.5"
  }
}
