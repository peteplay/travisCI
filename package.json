{
  "name": "travis-simple",
  "version": "1.0.0",
  "description": "Cypress Cucumber with Travis CI",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:electron": "cypress run --browser electron",
    "cy:run:edge": "cypress run --browser edge",
    "cy:dashboard": "cypress run --record --key f586ae59-974a-41f1-be19-943e4f84e12a",
    "cy:verify": "cypress verify",
    "cy:info": "cypress info",
    "start": "my-server -p 3030 --https",
    "cy:ci": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start https-get://localhost:3030 cy:run"
  },
  "keywords": [
    "TravisCI"
  ],
  "author": "Peter",
  "license": "ISC",
  "dependencies": {
    "cypress": "^5.5.0",
    "cypress-cucumber-preprocessor": "^4.0.0",
    "cypress-image-snapshot": "^3.1.1",
    "prettier": "^2.1.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "devDependencies": {
    "start-server-and-test": "^1.11.5"
  }
}
